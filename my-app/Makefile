# Makefile for setting up and running the React web application

# Define the default target
.DEFAULT_GOAL := run

# Install dependencies
install:
	@echo "Installing dependencies..."
	npm install

# Run the application
run:
	@echo "Starting the web application..."
	npm start

# Clean the build
clean:
	@echo "Cleaning up..."
	rm -rf node_modules
	rm -rf package-lock.json
